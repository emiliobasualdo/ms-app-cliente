{
  "name": "MasSimpleAppClient",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=qaDebug",
    "ios": "react-native run-ios --scheme qa",
    "start": "react-native start",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx",
    "coverage": "jest --coverage --passWithNoTests",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean",
    "force-clean": "yarn run android:clean && yarn run clean && rm -rf ios/build && rm -rf ios/Pods && rm -rf node_modules/ && yarn install && cd ios/ && pod install",
    "ios:clean": "cd ios/ && rm -rf build && rm -rf Pods",
    "android:clean": "cd android/ && ./gradlew clean",
    "android:build.qa": "cd android && ./gradlew clean && ./gradlew assembleQaRelease",
    "android:build.stage": "cd android && ./gradlew clean && ./gradlew assembleStageRelease",
    "android:build.production": "cd android && ./gradlew clean && ./gradlew bundleProductionRelease",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "lint-fix": "eslint src --ext .js,.ts,.jsx,.tsx --fix",
    "lint-diff": "git diff --name-only --cached --no-error-on-unmatched-pattern --relative | grep -E \"\\.(ts|tsx|js|jsx)$\" | xargs eslint",
    "tsc": "tsc"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/drawer": "^5.9.0",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "apisauce": "^1.1.2",
    "cerealizr": "^1.0.1-alpha",
    "formik": "^2.1.5",
    "i18next": "^19.6.3",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-config": "^1.3.3",
    "react-native-flipper": "0.47.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-localize": "^1.4.1",
    "react-native-reanimated": "^1.10.2",
    "react-native-safe-area-context": "^3.1.4",
    "react-native-screens": "^2.10.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swiper": "^1.6.0",
    "react-redux": "^7.2.1",
    "reactotron-apisauce": "^3.0.0",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "redux": "^4.0.5",
    "redux-recompose": "^2.2.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^3.3.0",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.44",
    "@types/react-native": "^0.63.4",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.3",
    "@types/seamless-immutable": "^7.1.13",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "babel-plugin-import-glob": "^2.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wolox-react-native": "^1.0.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-react-native": "^18.0.0",
    "jsdom": "^16.3.0",
    "jsdom-global": "^3.0.2",
    "metro-react-native-babel-preset": "^0.61.0",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "react-native-mock-render": "^0.1.9",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "16.13.1",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-diff && yarn run tsc"
    }
  }
}
